<%= form_with(model:@user,local:true) do |f| %>

<%= f.label :image %>
<%= image_tag(@user.image.url)if @user.image&&@user.image.url %>
<%= f.file_field :image %>
<%= f.hidden_field :image_cache %>

<%= f.label:name %>
<%= f.text_field:name %>

<%= f.label:email %>
<%= f.text_field:email %>

<%= f.label:password %>
<%= f.text_field:password%>

<%= f.label:password_confirmation %>
<%= f.text_field:password_confirmation %>

<%= f.submit "createuser" %>

<% end %>
